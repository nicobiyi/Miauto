<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_camionetasuperior"
    tools:context="com.example.nicolas.miauto.Activities.NeumaticosActivity">

    <TextView
        android:id="@+id/tvFecha1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/tvFecha2"
        android:layout_marginStart="36dp"
        android:text="Fecha 1" />

    <TextView
        android:id="@+id/tvFecha2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Fecha 2" />

    <TextView
        android:id="@+id/tvFecha3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="34dp"
        android:text="Fecha 3" />

    <TextView
        android:id="@+id/tvPressValuediH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="107dp"
        android:layout_marginTop="116dp"
        android:background="@color/colorPrimary"
        android:text="29" />

    <TextView
        android:id="@+id/tvPressValuetiH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tvPressValuediH"
        android:layout_marginBottom="116dp"
        android:background="@color/colorPrimary"
        android:text="30" />

    <TextView
        android:id="@+id/tvPressValueddH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tvPressValuediH"
        android:layout_marginEnd="108dp"
        android:background="@color/colorPrimary"
        android:text="30" />

    <TextView
        android:id="@+id/tvPressValuetdH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tvPressValuetiH"
        android:layout_alignEnd="@+id/tvPressValueddH"
        android:background="@color/colorPrimary"
        android:text="30" />

    <TextView
        android:id="@+id/tvPressValueauH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:background="@color/colorPrimary"
        android:text="33" />

    <ImageButton
        android:id="@+id/btnFecha1"
        android:layout_width="38dp"
        android:layout_height="41dp"
        android:layout_alignStart="@+id/tvFecha1"
        android:layout_below="@+id/tvFecha1"
        android:src="@drawable/ic_lupa" />

    <ImageButton
        android:id="@+id/btnFecha2"
        android:layout_width="37dp"
        android:layout_height="42dp"
        android:layout_below="@+id/tvFecha1"
        android:layout_centerHorizontal="true"
        android:src="@drawable/ic_lupa" />

    <ImageButton
        android:id="@+id/btnFecha3"
        android:layout_width="43dp"
        android:layout_height="41dp"
        android:layout_alignEnd="@+id/tvFecha3"
        android:layout_below="@+id/tvFecha1"
        android:src="@drawable/ic_lupa" />

    <TextView
        android:id="@+id/tvAviso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="NO HAY INFLADOS"
        android:textColor="#000000"
        android:visibility="invisible" />
</RelativeLayout>