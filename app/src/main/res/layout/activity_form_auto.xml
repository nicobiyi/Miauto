<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.InfoAutoActivity">

    <EditText
        android:id="@+id/etTipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="75dp"
        android:ems="10"
        android:inputType="text"
        android:hint="Sedan 5 Puertas"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/tvTipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/etTipo"
        android:layout_alignStart="@+id/tvMarca"
        android:text="@string/tipo"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="56dp" />

    <EditText
        android:id="@+id/etMotor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="129dp"
        android:ems="10"
        android:hint="AKY876578"
        android:inputType="text"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/tvMotor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvTipo"
        android:layout_alignTop="@+id/etMotor"
        android:text="@string/text_motor"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="56dp" />

    <EditText
        android:id="@+id/etModelo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="201dp"
        android:ems="10"
        android:inputType="text"
        android:hint="Suran 1.6 5D 758"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/tvModelo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/etModelo"
        android:layout_alignStart="@+id/tvTipo"
        android:text="@string/text_modelo"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="56dp" />

    <TextView
        android:id="@+id/tvMarca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/etMarca"
        android:layout_alignStart="@+id/tvPatente2"
        android:text="@string/text_marca"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="56dp" />

    <EditText
        android:id="@+id/etChasis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="197dp"
        android:ems="10"
        android:inputType="text"
        android:hint="8WAPM45ZE3657654"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="40dp" />

    <EditText
        android:id="@+id/etMarca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="119dp"
        android:ems="10"
        android:inputType="text"
        android:hint="VolksWagen"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="40dp" />

    <TextView
        android:id="@+id/tvPatente2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/etPatente3"
        android:layout_marginStart="46dp"
        android:text="@string/text_patente"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="56dp" />

    <TextView
        android:id="@+id/tvChasis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/etChasis"
        android:layout_alignStart="@+id/tvTipo"
        android:text="@string/text_chasis"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="56dp" />

    <EditText
        android:id="@+id/etPatente3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="46dp"
        android:ems="10"
        android:inputType="text"
        android:hint="ABC123"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="40dp" />

    <Button
        android:id="@+id/btnRegistrarAuto"
        android:layout_width="383dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-153dp"
        android:layout_toEndOf="@+id/tvChasis"
        android:text="@string/btn_registrar_auto" />
</RelativeLayout>