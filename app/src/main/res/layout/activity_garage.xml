<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallp"
    tools:context="com.example.nicolas.miauto.Activities.GarageActivity">

    <ImageView
        android:id="@+id/btnSelectAuto"
        android:layout_width="301dp"
        android:layout_height="168dp"
        android:layout_above="@+id/btnBorrarAuto"
        android:layout_centerHorizontal="true"
        android:src="@drawable/patenteenblanco"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="291dp" />

    <ImageButton
        android:id="@+id/btnBorrarAuto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:src="@android:drawable/ic_delete"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent" />

    <TextView
        android:id="@+id/tvPatente"
        android:layout_width="275dp"
        android:layout_height="56dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="96dp"
        android:textAlignment="center"
        android:textSize="40sp"
        android:textStyle="bold" />

</RelativeLayout>
